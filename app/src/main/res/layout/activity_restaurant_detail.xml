<androidx.coordinatorlayout.widget.CoordinatorLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    >

    <com.google.android.material.appbar.AppBarLayout
        android:id="@+id/restaurantA_appbar"
        android:layout_width="match_parent"
        android:layout_height="350dp"
        android:background="#F5F5F5"
        app:elevation="0dp"
        >

        <com.google.android.material.appbar.CollapsingToolbarLayout
            android:id="@+id/main.collapsing"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_scrollFlags="scroll|exitUntilCollapsed"
            android:background="@color/white"
            app:titleEnabled="false"
            app:contentScrim="@android:color/transparent"
            >
            <ImageView
                android:id="@+id/restaurantA.cover_image"
                android:layout_width="match_parent"
                android:layout_height="175dp"
                app:layout_collapseMode="parallax"
                android:src="@drawable/food_location_cover_image"
                android:scaleType="centerCrop"
                />
            <LinearLayout
                android:id="@+id/restaurant_header_content"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/restaurantA.cover_image"
                android:orientation="vertical"
                android:paddingLeft="15dp"
                android:paddingRight="15dp"
                android:layout_marginTop="175dp"
                app:layout_collapseMode="parallax"
                >

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:paddingTop="15dp"
                    android:paddingBottom="10dp">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="4dp"
                        android:fontFamily="@font/fontawesome_pro_solid900"
                        android:text="\uf2f7"
                        android:textColor="#FFC107"
                        android:textSize="20sp" />

                    <TextView
                        android:id="@+id/restaurantA.txv_restaurant_name"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="\t\t\t\t"
                        android:textColor="@color/black"
                        android:textSize="22sp"
                        android:textStyle="bold" />
                </RelativeLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:paddingTop="10dp"
                    android:paddingBottom="15dp">

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:fontFamily="@font/fontawesome_pro_solid900"
                            android:gravity="center_vertical"
                            android:paddingRight="5dp"
                            android:text="\uf005"
                            android:textColor="#FFC107"
                            android:textSize="16dp" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:fontFamily="@font/fontawesome_pro_solid900"
                            android:gravity="center_vertical"
                            android:paddingRight="5dp"
                            android:text="\uf005"
                            android:textColor="#FFC107"
                            android:textSize="16dp" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:fontFamily="@font/fontawesome_pro_solid900"
                            android:gravity="center_vertical"
                            android:paddingRight="5dp"
                            android:text="\uf005"
                            android:textColor="#FFC107"
                            android:textSize="16dp" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:fontFamily="@font/fontawesome_pro_solid900"
                            android:gravity="center_vertical"
                            android:paddingRight="5dp"
                            android:text="\uf005"
                            android:textColor="#FFC107"
                            android:textSize="16dp" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:fontFamily="@font/fontawesome_pro_light300"
                            android:gravity="center_vertical"
                            android:paddingRight="5dp"
                            android:text="\uf005"
                            android:textColor="#FFC107"
                            android:textSize="16dp" />

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="4.8 (500+ Bình luận)"
                            android:textColor="@color/black" />
                    </LinearLayout>

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/fontawesome_pro_light300"
                        android:text="\uf004"
                        android:textAlignment="textEnd"
                        android:textSize="25dp" />

                </LinearLayout>

            </LinearLayout>





            <androidx.appcompat.widget.Toolbar
                android:id="@+id/restaurantA_toolbar"
                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:minHeight="?attr/actionBarSize"
                android:background="@android:color/transparent"
                android:elevation="0dp"
                app:contentInsetEnd="0dp"
                app:contentInsetStart="0dp"
                app:layout_collapseMode="pin"

                >
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:weightSum="1"
                    >
                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="0.15"
                        android:gravity="center_horizontal"
                        >
<!--                        <com.google.android.material.button.MaterialButton-->
<!--                            android:layout_width="35dp"-->
<!--                            android:layout_height="35dp"-->
<!--                            style="@style/Button.CrystalClear"-->
<!--                            app:icon="@drawable/ic_arrow_left_solid"-->
