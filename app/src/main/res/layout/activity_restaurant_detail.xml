<!--                            />-->
                        <EditText
                            android:id="@+id/restaurantA.edt_search"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:background="#f0f0f0"
                            android:drawableStart="@drawable/ic_lookup_light"
                            android:textSize="16sp"
                            android:drawablePadding="5dp"
                            android:drawableTint="#252525"
                            android:hint="Tìm món tại..."
                            android:paddingLeft="10dp"
                            android:textColorHint="#747474"
                            android:visibility="invisible" />

                        <com.google.android.material.button.MaterialButton
                            android:id="@+id/restaurantA.btn_search"
                            style="@style/Button.CrystalClear"
                            android:layout_width="40dp"
                            android:layout_height="40dp"
                            android:layout_alignParentRight="true"
                            android:layout_marginTop="5dp"
                            android:insetLeft="0dp"
                            android:insetTop="0dp"
                            android:insetRight="0dp"
                            android:insetBottom="0dp"
                            android:visibility="visible"
                            app:icon="@drawable/ic_lookup_light"
                            app:iconGravity="textStart"
                            app:iconPadding="0dp"
                            app:iconSize="30sp"
                            app:iconTint="@color/white" />
                    </RelativeLayout>


                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="0.15"
                        android:gravity="center_horizontal"
                        >
                        <com.google.android.material.button.MaterialButton
                            android:id="@+id/restaurantA.btn_share"
                            android:layout_width="40dp"
                            android:layout_height="40dp"
                            style="@style/Button.CrystalClear"
                            app:icon="@drawable/ic_share_light"
                            android:insetLeft="0dp"
                            android:insetBottom="0dp"
                            android:insetRight="0dp"
                            android:insetTop="0dp"
                            app:iconPadding="0dp"
                            app:iconGravity="textStart"
                            app:iconTint="@color/white"
                            app:iconSize="38sp"
                            android:layout_gravity="center_vertical"
                            />
                    </LinearLayout>

                </LinearLayout>



